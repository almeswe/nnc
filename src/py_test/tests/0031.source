fn main(): i32 {
    let arr: i32[2];
    let p: i32*;
    arr[0] = 2;
    arr[1] = 3;
    p = &arr[0];

	if(*(p) != 2) {
		return 1;
    }
    p = p + 1;

	if(*(p) != 3) {
		return 2;
    }
    p = p + 1;

	p = &arr[1];
	if(*(p) != 3) {
		return 1;
    }
    p = p - 1;

	if(*(p) != 2) {
		return 2;
    }
    p = p - 1;
		
	p = &arr[0];
    p = p + 1;
	if(*(p) != 3) {
		return 1;
    }
	
	p = &arr[1];
    p = p - 1;
	if(*(p) != 2) {
		return 1;
    }
    return 0;
}